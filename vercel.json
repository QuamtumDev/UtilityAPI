{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/",
          "dest": "/index.js"
        },
        {
          "src": "/values",
          "dest": "/values.json"
        },
        {
          "src": "/json",
          "dest": "/use.json"
        }
    ]
  }
  