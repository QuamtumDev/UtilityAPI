{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
        {
          "source": "/",
          "destination": "index.js"
        },
        {
          "source": "/values",
          "destination": "values.json"
        },
        {
          "source": "/json",
          "destination": "use.json"
        }
    ]
  }
  